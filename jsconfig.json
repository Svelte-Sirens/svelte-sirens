{
	"compilerOptions": {
		"baseUrl": ".",
		"paths": {
			"@components": ["src/lib/components"],
			"@layout": ["src/lib/layout"],
			"@lib": ["src/lib"],
			"@lib/*": ["src/lib/*"],
			"@utils": ["src/lib/utils"],
			"@styles": ["src/lib/styles"],
			"@stores": ["src/lib/stores"]
		}
	},
	"include": ["src/**/*.d.ts", "src/**/*.js", "src/**/*.svelte"]
}
