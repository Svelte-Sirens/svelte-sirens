{
	"$schema": "./node_modules/wrangler/config-schema.json",
	"name": "website",
	"main": ".svelte-kit/cloudflare/_worker.js",
	"compatibility_date": "2025-11-25",
	"compatibility_flags": ["nodejs_als"],
	"assets": {
		"binding": "ASSETS",
		"directory": ".svelte-kit/cloudflare",
	},
	"workers_dev": false,
	"preview_urls": true,
	"routes": [
		{
			"custom_domain": true,
			"pattern": "sveltesirens.dev",
		},
	],
}
